    <owl:Ontology rdf:about="https://w3id.org/rec/dc/">
        <owl:versionIRI rdf:resource="https://w3id.org/rec/dc/1.1/"/>
        <dc:source rdf:resource="http://bloody-byte.net/rdf/dc_owl2dl/"/>
        <rdfs:comment xml:lang="en">This is a translation of the Dublin Core Metadata Element Set into OWL annotation properties.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/elements/1.1/"/>
        <rdfs:label xml:lang="en">Dublin Core Metadata Element Set, Version 1.1</rdfs:label>
    </owl:Ontology>
    


