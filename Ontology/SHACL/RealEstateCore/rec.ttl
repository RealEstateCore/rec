      sh:name "turnover date" ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path rec:weight ;
      sh:datatype xsd:decimal ;
      sh:maxCount 1 ;
      sh:name "weight" ;
    ] ;
